{
  "name": "python-webpack-boilerplate",
  "version": "1.0.0",
  "description": "Webpack boilerplate for Django & Flask",
  "scripts": {
    "build": "node node_modules/cross-env/src/bin/cross-env.js NODE_ENV=production node_modules/webpack/bin/webpack.js --config frontend/webpack/webpack.config.prod.js",
    "start": "webpack serve --config frontend/webpack/webpack.config.dev.js",
    "watch": "webpack --watch --config frontend/webpack/webpack.config.watch.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AccordBox/python-webpack-boilerplate"
  },
  "keywords": [
    "webpack",
    "startkit",
    "frontend"
  ],
  "author": "Michael Yin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AccordBox/python-webpack-boilerplate/issues"
  },
  "devDependencies": {},
  "dependencies": {
    "@babel/core": "^7.23.0",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.22.20",
    "babel-loader": "^8.3.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.0",
    "core-js": "^3.33.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "eslint-webpack-plugin": "^3.2.0",
    "materialize-css": "^1.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.8.3",
    "sass": "^1.69.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.3",
    "stylelint": "^14.16.1",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-webpack-plugin": "^3.3.0",
    "webpack": "^5.88.2",
    "webpack-assets-manifest": "^5.1.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  }
}
